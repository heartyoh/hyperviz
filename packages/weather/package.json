{
  "name": "@hyperviz/weather",
  "version": "0.0.1",
  "description": "Weather data visualization module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "browser": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "author": "heartyoh@hatiolab.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@hyperviz:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperviz/weather.git",
    "directory": "packages/weather"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build:examples": "tsc -p tsconfig.examples.json",
    "start:examples": "npm run build:examples && npx serve ../..",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsc -w"
  },
  "keywords": [
    "weather",
    "visualization",
    "offscreencanvas",
    "webworker"
  ],
  "dependencies": {
    "@hyperviz/worker": "^0.1.8",
    "axios": "^1.0.0",
    "canvas": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
