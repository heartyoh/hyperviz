{
  "name": "@hyperviz/weather",
  "version": "0.2.7",
  "description": "Weather visualization module using OffscreenCanvas and Web Workers",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "browser": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "author": "heartyoh@hatiolab.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@hyperviz:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperviz/weather.git",
    "directory": "packages/weather"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build:examples": "tsc -p tsconfig.examples.json",
    "start:examples": "npm run build:examples && npx serve ../..",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsc -w"
  },
  "keywords": [
    "weather",
    "visualization",
    "offscreencanvas",
    "webworker"
  ],
  "dependencies": {
    "@hyperviz/worker": "^0.2.7",
    "ol": "^10.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.14.1",
    "jest": "^29.5.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
